{
  "name": "grade-browser",
  "version": "1.0.0",
  "description": "Simple web application to browse AutoLab grading reports",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "docker:build": "docker build -t grade-browser .",
    "docker:run": "docker run -p 3000:3000 grade-browser",
    "docker:dev": "npm run docker:build && npm run docker:run",
    "install-deps": "npm install",
    "clean": "rm -rf node_modules package-lock.json",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "chokidar": "^3.5.3",
    "ws": "^8.14.2"
  },
  "keywords": [
    "autolab",
    "grading",
    "browser"
  ],
  "author": "Marcus Rosado",
  "license": "MIT"
}